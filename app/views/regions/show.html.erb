<h1>Region: <%= @region.name.capitalize %></h1>
<img src="<%=@region.image_url %>" alt="<%=@region.name %>" width = 500>
<br>
<p class = "mainthick"><%= @region.begDate %> - <%= @region.endDate %></p>
<br>
<!-- BUTTONS -->
<a href="/<%=@region.name%>/edit">Edit</a>
<a href="/destroy_region/<%= @region.name.downcase %>">Destroy</a>
<br>
<br>
<h4 class = "maintheme"><%= link_to "+ See Locations Visited in " + @region.name.capitalize, "/regions/:name/locations" %></h3>
